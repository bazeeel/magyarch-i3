#!/bin/sh

# This file runs when a DM logs you into a graphical session.


setbg &     # Wallpaper changer script

sxhkd &     # Simple X hotkey daemon for setting up keybinds

mpd &       # Music player daemon 

xsetroot -cursor_name left_ptr &   # Change cursor from default x shape

xset r rate 300 50 &	# Speed xrate up

#xrdb .Xresources &       

picom --config ~/.config/picom.conf &	# compositor 

dunst &                                # notification daemon

exec --no-startup-id unclutter &       # hide mouse pointer when idle

exec --no-startup-id nm-applet &        # networkmanager applet/wifi,ethernet settings

/usr/lib/polkit-gnome/polkit-gnome-authentication-agent-1 &

zathura ~/.local/bin/Magyarch-i3.pdf &
